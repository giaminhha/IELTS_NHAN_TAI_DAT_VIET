      <ns0:item ident="text2qti_text_3f62ab36177d38854c14aba3d52d0421b1ab713ecaf157b11129c9c70a72b3ef" title="Water scarcity and management">
        <ns0:itemmetadata>
          <ns0:qtimetadata>
            <ns0:qtimetadatafield>
              <ns0:fieldlabel>question_type</ns0:fieldlabel>
              <ns0:fieldentry>text_only_question</ns0:fieldentry>

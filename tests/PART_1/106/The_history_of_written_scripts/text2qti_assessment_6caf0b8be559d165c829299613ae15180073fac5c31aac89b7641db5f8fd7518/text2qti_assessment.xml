      <item ident="text2qti_text_da70e5cdd3c37ef0c4f97f69bd5298dc6f89df6e1d12dc1e994626ed0c767497" title="The history of written scripts">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>text_only_question</fieldentry>
            </qtimetadatafield>
